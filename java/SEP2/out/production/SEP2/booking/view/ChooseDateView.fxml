<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import booking.view.DateTimePicker?>
<?import javafx.scene.shape.MeshView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="booking.view.ChooseDateView"
            prefHeight="400.0" prefWidth="600.0">
    <VBox>
        <DatePicker fx:id="datePicker"/>
        <HBox>
            <Label text="Til"/>
            <TextField fx:id="timeTextFrom"/>
        </HBox>
        <HBox>
            <Label text="Fra"/>
            <TextField fx:id="timeTextTo"/>
        </HBox>
        <Button text="Se lokaler" onAction="#showRoomsClicked"/>
    </VBox>

</AnchorPane>
